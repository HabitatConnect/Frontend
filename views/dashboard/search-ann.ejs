<div class="container-fluid container-fluid-custom pb-5 mb-5">
    <div class="row">
        <% if(searchResults != '') { %>
            <% searchResults.forEach(function(announcement, index) { %>
                <a href="/dashboard/item/<%= announcement._id %>">
                    <h4><%= announcement.title %></h4>
                </a>
            <% }) %>
        <% } else { %>
            <p>No items found</p>
        <% } %>
    </div>
</div>